@model PagedList.IPagedList<SnifferManager.Models.ViewModel.LogViewModel>
@using PagedList.Mvc;

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Арендатор</th>
            <th>SerialNumber</th>
            <th>Получен</th>
            <th>Тип</th>
            <th>Сообщение</th>
            <th>Серверное время</th>
            <th>Системное время</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model)
        {
            <tr>
                <td>@log.id</td>
                <td>@log.LocatioName</td>
                <td>@log.SerialNumber</td>
                <td>@log.ReceiveDate</td>
                <td>@log.LogType</td>
                <td>@log.Message</td>
                <td>@log.ServerTime</td>
                <td>@log.SystemTime</td>
            </tr>
        }
    </tbody>
</table>

@Html.PagedListPager(Model, page => Url.Action("Index", new { page }))